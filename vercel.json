{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/gateway/dist/index.js"
        },
        {
            "source": "/api/transactions/(.*)",
            "destination": "/services/transactions/dist/index.js"
        }
    ],
    "functions": {
        "src/api/gateway/dist/index.js": {
            "runtime": "nodejs18.x"
        },
        "services/transactions/dist/index.js": {
            "runtime": "nodejs18.x"
        }
    }
}