{
  "name": "@nexidus/dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "yarn dev",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build-docker": "docker buildx build --platform linux/arm64/v8 --tag nexidus-frontend-dashboard:latest --load .",
    "build:docker": "docker build . -t \"nexidus-dashboard:v1.0\"",
    "build:docker:local": "docker build . -f Dockerfile.local --platform linux/amd64 --tag nexidus-local:latest --load .",
    "build:docker:prod": "docker build . -f Dockerfile.prod --platform linux/amd64 --tag nexidus-prod:latest --load .",
    "start:docker": "docker run -d -p 8080:8080 --name nexidus-frontend-dashboard nexidus-frontend-dashboard:latest"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@tailwindcss/vite": "^4.0.3",
    "@tanstack/react-table": "^8.20.6",
    "ag-grid-enterprise": "^33.0.4",
    "ag-grid-react": "^33.0.4",
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "date-fns": "^4.1.0",
    "i18next": "^24.2.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^8.0.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^7.1.5",
    "recharts": "^2.15.1",
    "tailwindcss": "^4.0.3",
    "victory": "^37.3.6",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
